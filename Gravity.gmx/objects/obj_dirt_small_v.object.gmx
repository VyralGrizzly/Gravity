            <string>
//platform movement

if ( step &gt; 0){

    step -= 1;
    y += vsp;
    
    }
    else{ 
    
        step = distance;
        vsp *= -1
        
        //below section used to get rid of a weird glitch with player getting stuck
        if (place_meeting(x,y-3,obj_player_char)|| place_meeting(x,y+3,obj_player_char)){   
        
            if (global.flip = 1) obj_player_char.y -= 2
            if (global.flip = -1) obj_player_char.y += 2
        
            }       
        }
        
// player standing on platform
vsp_carry = 0;


if (global.flip = 1){

    if (place_meeting(x,y-3,obj_player_char)){   
        
        vsp_carry = vsp;
        obj_player_char.y += vsp_carry;
        }   
        
    if (place_meeting(x,y+10,obj_player_char)){   
        
        obj_player_char.vsp = 0;
        obj_player_char.vsp += 3;
        }   
        
    }
    
if (global.flip = -1){

    if (place_meeting(x,y+3,obj_player_char)){   
        
        vsp_carry = vsp;
        obj_player_char.y += vsp_carry;
        
        }
    if (place_meeting(x,y-10,obj_player_char)){   
        
        obj_player_char.vsp = 0;
        obj_player_char.vsp -= 3;
        }      
    }
</string>
